{
  "name": "maroonrides",
  "version": "1.0.0",
  "main": "Main.tsx",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "expo start --dev-client",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@bacons/apple-targets": "^3.0.2",
    "@bacons/text-decoder": "^0.0.0",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@googlemaps/polyline-codec": "^1.0.28",
    "@gorhom/bottom-sheet": "^5.2.4",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@tanstack/query-async-storage-persister": "^5.83.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-persist-client": "^5.83.0",
    "aggie-spirit-api": "^1.3.4",
    "expo": "^54.0.7",
    "expo-build-properties": "~1.0.8",
    "expo-constants": "~18.0.8",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "moment": "^2.30.1",
    "moment-strftime": "^0.5.0",
    "patch-package": "^8.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-timeline-flatlist": "^0.8.0",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "0.5.1",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.13",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@bacons/apple-targets",
          "@bacons/text-decoder",
          "@googlemaps/polyline-codec",
          "aggie-spirit-api",
          "moment",
          "moment-strftime",
          "patch-package",
          "react-native-timeline-flatlist"
        ]
      }
    }
  }
}
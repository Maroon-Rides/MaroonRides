{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "always"
  },
  "eslint.rules.customizations": [
    {
      "rule": "prettier/prettier",
      "severity": "off"
    }
  ],
  "editor.tabSize": 2,
  "editor.formatOnSave": true
}